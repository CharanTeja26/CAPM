using CHARAN_C_REGULATORY_STATUSES_TEXTS as CHARAN_C_REGULATORY_STATUSES_TEXTS;
entity CHARAN_C_REGULATORY_STATUSES {
  key ID : Decimal(4, 0);
  APP_NAME : String(64);
  DESCRIPTION : String(100);
  TEXTS : association[*] to CHARAN_C_REGULATORY_STATUSES_TEXTS on TEXTS.ID = ID;
  LOCALIZED : association to CHARAN_C_REGULATORY_STATUSES_TEXTS on LOCALIZED.ID = ID and LOCALIZED.LOCALE = SESSION_CONTEXT('LOCALE');
};
