using LOCALIZED_CHARAN_REGULATORY_NOTICE_REQUEST_HEADER as LOCALIZED_CHARAN_REGULATORY_NOTICE_REQUEST_HEADER;
using LOCALIZED_CHARAN_C_REGULATORY_STATUSES as LOCALIZED_CHARAN_C_REGULATORY_STATUSES;
using CHARAN_MID_OFFICE_OWNERS as CHARAN_MID_OFFICE_OWNERS;
using CHARAN_SUPPLIERGROUPS as CHARAN_SUPPLIERGROUPS;
using CHARAN_RN_SUPPLIER_GROUP_MASTER_DATA as CHARAN_RN_SUPPLIER_GROUP_MASTER_DATA;
using LOCALIZED_SAP_COMMON_CURRENCIES as LOCALIZED_SAP_COMMON_CURRENCIES;
view LOCALIZED_VIEWS_SMART_REGULATORY_NOTICE_MASTER as select from ((((LOCALIZED_CHARAN_REGULATORY_NOTICE_REQUEST_HEADER as REGULATORY_NOTICE_REQUEST_HEADER_0 left join LOCALIZED_CHARAN_C_REGULATORY_STATUSES as LINK_TO_STATUS_1 on LINK_TO_STATUS_1.ID = REGULATORY_NOTICE_REQUEST_HEADER_0.RN_SIGNOFF_STATUS) left join CHARAN_MID_OFFICE_OWNERS as LINK_TO_MID_OFFICE_OWNER_2 on LINK_TO_MID_OFFICE_OWNER_2.MOO_ID = REGULATORY_NOTICE_REQUEST_HEADER_0.MOO_ID) left join CHARAN_SUPPLIERGROUPS as LINK_TO_SUPPLIERGROUPS_3 on LINK_TO_SUPPLIERGROUPS_3.YSGROUP = REGULATORY_NOTICE_REQUEST_HEADER_0.SUPPLIER_GROUP_CODE) left join CHARAN_RN_SUPPLIER_GROUP_MASTER_DATA as LINK_TO_SUPP_GROUP_MASTER_4 on LINK_TO_SUPP_GROUP_MASTER_4.SUPPLIERGROUPCODE = REGULATORY_NOTICE_REQUEST_HEADER_0.SUPPLIER_GROUP_CODE) mixin {
  ___ANNUALSPENDCURRENCY : association to LOCALIZED_SAP_COMMON_CURRENCIES on ___ANNUALSPENDCURRENCY.CODE = $projection.ANNUALSPENDCURRENCY_CODE;
} into {
  key REGULATORY_NOTICE_REQUEST_HEADER_0.RN_UUID as RN_UUID,
  key REGULATORY_NOTICE_REQUEST_HEADER_0.RN_ID as RN_ID,
  REGULATORY_NOTICE_REQUEST_HEADER_0.RN_COLLECTION_WAVE,
  REGULATORY_NOTICE_REQUEST_HEADER_0.RN_SIGNOFF_STATUS,
  LINK_TO_STATUS_1.DESCRIPTION as STATUSDESCRIPTION,
  REGULATORY_NOTICE_REQUEST_HEADER_0.RN_SENT_OUT,
  REGULATORY_NOTICE_REQUEST_HEADER_0.RN_SENT_OUT_DATE,
  REGULATORY_NOTICE_REQUEST_HEADER_0.RN_SIGN_DATE,
  REGULATORY_NOTICE_REQUEST_HEADER_0.RN_TARGET_DEADLINE_DATE,
  REGULATORY_NOTICE_REQUEST_HEADER_0.LEAD_SPEND_CATEGORY,
  REGULATORY_NOTICE_REQUEST_HEADER_0.LEAD_SPEND_MAT_GROUP,
  REGULATORY_NOTICE_REQUEST_HEADER_0.ANNUAL_SPEND_VALUE_AMOUNT as ANNUALSPENDVALUE,
  ___ANNUALSPENDCURRENCY as ANNUALSPENDCURRENCY,
  REGULATORY_NOTICE_REQUEST_HEADER_0.ANNUAL_SPEND_VALUE_CURRENCY_CODE as ANNUALSPENDCURRENCY_CODE,
  REGULATORY_NOTICE_REQUEST_HEADER_0.MOO_ID,
  LINK_TO_MID_OFFICE_OWNER_2.NAME as MIDOFFICEOWNERNAME,
  LINK_TO_MID_OFFICE_OWNER_2.EMAIL as MIDOFFICEOWNEREMAIL,
  REGULATORY_NOTICE_REQUEST_HEADER_0.SUPPLIER_GROUP_CODE,
  LINK_TO_SUPPLIERGROUPS_3.YSGRTXT as SUPPLIERGROUPNAME,
  LINK_TO_SUPP_GROUP_MASTER_4.SUPPLIERCONTACTNAME as SUPPLIERCONTACTNAME,
  LINK_TO_SUPP_GROUP_MASTER_4.SUPPLIEREMAIL as SUPPLIEREMAIL,
  LINK_TO_SUPP_GROUP_MASTER_4.SUPPLIERPHONE as SUPPLIERCONTACTNO,
  LINK_TO_SUPP_GROUP_MASTER_4.FPSUPPLIERNAME as FPSUPPLIERNAME,
  LINK_TO_SUPP_GROUP_MASTER_4.FPSUPPLIEREMAIL as FPSUPPLIEREMAIL,
  LINK_TO_SUPP_GROUP_MASTER_4.SUPPLIERNAME_3PM as SUPPLIERNAME3PM,
  LINK_TO_SUPP_GROUP_MASTER_4.SUPPLIEREMAIL_3PM as SUPPLIEREMAIL3PM,
  REGULATORY_NOTICE_REQUEST_HEADER_0.HISTORY_CRTD_BY as CRTD_BY,
  REGULATORY_NOTICE_REQUEST_HEADER_0.HISTORY_CRTD_DATE as CRTD_DATE,
  REGULATORY_NOTICE_REQUEST_HEADER_0.HISTORY_CHNG_BY as CHNG_BY,
  REGULATORY_NOTICE_REQUEST_HEADER_0.HISTORY_CHNG_DATE as CHNG_DATE
};
